{
  "name": "Chuchotriz",
  "short_name": "ChuchoTriz",
  "description": "Agrega funcionalidad en Matriz",
  "version": "0.0.1",
  "author": "Chucho Trader",
  "homepage_url": "https://x.com/chuchotrader",
  "manifest_version": 3,
  "options_page": "index.html",
  "options_ui": {
    "page": "index.html",
    "open_in_tab": false
  },
  "icons": {
    "48": "logo48.png",
    "64": "logo64.png",
    "128": "logo128.png",
    "512": "logo512.png"
  },
  "action": {
    "default_title": "Popup",
    "default_popup": "index.html",
    "default_icon": {
      "48": "logo48.png",
      "64": "logo64.png",
      "128": "logo128.png",
      "512": "logo512.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://*.xoms.com.ar/*"],
      "js": ["src/main.tsx"],
      "media": [],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ]
}
